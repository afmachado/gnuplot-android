<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/keyboard_backcolor"
    android:keyTextColor="#FFFF0000" >

    <Row
        android:horizontalGap="0.5%p"
        android:keyHeight="7%p"
        android:keyWidth="9.4%p"
        android:verticalGap="0.5%p" >
        <Key
            android:keyLabel="1"
            android:keyOutputText="1" />
        <Key
            android:keyLabel="2"
            android:keyOutputText="2" />
        <Key
            android:keyLabel="3"
            android:keyOutputText="3" />
        <Key
            android:keyLabel="4"
            android:keyOutputText="4" />
        <Key
            android:keyLabel="5"
            android:keyOutputText="5" />
        <Key
            android:keyLabel="6"
            android:keyOutputText="6" />
        <Key
            android:keyLabel="7"
            android:keyOutputText="7" />
        <Key
            android:keyLabel="8"
            android:keyOutputText="8" />
        <Key
            android:keyLabel="9"
            android:keyOutputText="9" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="0"
            android:keyOutputText="0" />
    </Row>
    <Row
        android:horizontalGap="0.5%p"
        android:keyHeight="7%p"
        android:keyWidth="9.4%p"
        android:verticalGap="0.5%p" >
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="!"
            android:keyOutputText="!" />
        <Key
            android:keyLabel="\@"
            android:keyOutputText="\@" />
        <Key
            android:keyLabel="#"
            android:keyOutputText="#" />
        <Key
            android:keyLabel="$"
            android:keyOutputText="$" />
        <Key
            android:keyLabel="%"
            android:keyOutputText="%" />
        <Key
            android:keyLabel="^"
            android:keyOutputText="^" />
        <Key
            android:keyLabel="&amp;"
            android:keyOutputText="&amp;" />
        <Key
            android:keyLabel="*"
            android:keyOutputText="*" />
        <Key
            android:keyLabel="( )"
            android:keyOutputText="()"
            android:popupKeyboard="@xml/parentheses" />
        <Key
            android:keyLabel="="
            android:keyOutputText="=" />
    </Row>
    <Row
        android:horizontalGap="0.5%p"
        android:keyHeight="7%p"
        android:keyWidth="9.4%p"
        android:verticalGap="0.5%p" >
        <Key
            android:keyLabel="("
            android:keyOutputText="(" />
        <Key
            android:keyLabel=")"
            android:keyOutputText=")" />
        <Key
            android:keyLabel="e"
            android:keyOutputText="e" />
        <Key
            android:keyLabel="&quot;"
            android:keyOutputText="&quot;" />
        <Key
            android:keyLabel="&quot; &quot;"
            android:keyOutputText="&quot;&quot;" />
        <Key
            android:keyLabel="PI"
            android:keyOutputText="PI" />
        <Key
            android:keyLabel="-"
            android:keyOutputText="-" />
        <Key
            android:keyLabel="+"
            android:keyOutputText="+" />
        <Key
            android:keyLabel="{ }"
            android:keyOutputText="{}"
            android:popupKeyboard="@xml/parentheses" />
        <Key
            android:keyLabel="_"
            android:keyOutputText="_" />
    </Row>
    <Row
        android:horizontalGap="0.5%p"
        android:keyHeight="7%p"
        android:keyWidth="9.4%p"
        android:verticalGap="0.5%p" >
        <Key
            android:horizontalGap="2%p"
            android:keyEdgeFlags="left"
            android:keyLabel="{"
            android:keyOutputText="{" />
        <Key
            android:keyLabel="}"
            android:keyOutputText="}" />
        <Key
            android:keyLabel=":"
            android:keyOutputText=":" />
        <Key
            android:keyLabel=";"
            android:keyOutputText=";" />
        <Key
            android:keyLabel="&apos;"
            android:keyOutputText="&apos;" />
        <Key
            android:keyLabel="&apos; &apos;"
            android:keyOutputText="&apos; &apos;" />
        <Key
            android:keyLabel="|"
            android:keyOutputText="|" />
        <Key
            android:keyLabel="\\"
            android:keyOutputText="\\" />
        <Key
            android:keyLabel="[ ]"
            android:keyOutputText="[]"
            android:popupKeyboard="@xml/parentheses" />
        <Key
            android:codes="67"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="8%p" />
    </Row>
    <Row
        android:horizontalGap="0.5%p"
        android:keyHeight="7%p"
        android:keyWidth="9.4%p"
        android:verticalGap="0.5%p" >
        <Key
            android:horizontalGap="4%p"
            android:keyEdgeFlags="left"
            android:keyLabel="["
            android:keyOutputText="[" />
        <Key
            android:keyLabel="]"
            android:keyOutputText="]" />
        <Key
            android:keyLabel="~"
            android:keyOutputText="~" />
        <Key
            android:keyLabel="&lt;"
            android:keyOutputText="&lt;" />
        <Key
            android:keyLabel=">"
            android:keyOutputText=">" />
        <Key
            android:keyLabel="\?"
            android:keyOutputText="\?" />
        <Key
            android:keyLabel="/"
            android:keyOutputText="/" />
        <Key
            android:codes="-3"
            android:keyLabel="↑"
            android:keyWidth="6%p" />
        <Key
            android:keyLabel=";"
            android:keyOutputText=";"
            android:keyWidth="6%p" />
        <Key
            android:codes="66"
            android:horizontalGap="1.5%p"
            android:keyHeight="15%p"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="10.5%p"
            android:verticalGap="1.5%p" />
    </Row>
    <Row
        android:horizontalGap="0.5%p"
        android:keyHeight="7%p"
        android:keyWidth="9.4%p"
        android:verticalGap="0.5%p" >
        <Key
            android:codes="-196"
            android:keyLabel="1 2 3"
            android:keyWidth="10%p" />
        <Key
            android:codes="-121"
            android:keyLabel="a b c"
            android:keyWidth="15%p" />
        <Key
            android:codes="62"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="31%p" />
        <Key
            android:keyLabel=","
            android:keyOutputText=","
            android:keyWidth="4%p" />
        <Key
            android:keyLabel="."
            android:keyOutputText="."
            android:keyWidth="4%p"
            android:popupKeyboard="@xml/dots" />
        <Key
            android:codes="-1"
            android:isRepeatable="true"
            android:keyLabel="←"
            android:keyWidth="6%p" />
        <Key
            android:codes="-4"
            android:keyLabel="↓"
            android:keyWidth="6%p" />
        <Key
            android:codes="-2"
            android:isRepeatable="true"
            android:keyLabel="→"
            android:keyWidth="6%p"
            android:verticalGap="5%p" />
    </Row>

</Keyboard>